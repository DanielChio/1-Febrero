// Hora N5
// 2 Circulo que rebota a lo ancho de la pantalla
var x=200;
var vel=5;
function setup() {
  createCanvas(400,400);
}

function draw() {
    background(0);
    fill(255,0,0);
    ellipse(x,200,100);
    x = x + vel;
    if(x>350){
        vel = vel*-1
    }
    if(x<50){
        vel = vel*-1
}
}
